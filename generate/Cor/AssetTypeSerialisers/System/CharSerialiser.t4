    class CharSerialiser 
        : AssetTypeSerialiser<Char>
    {
        internal CharSerialiser () {}

        protected internal override Char Read(AssetBinaryReader abr)
        {
            return abr.ReadChar ();
        }

        protected internal override void Write(AssetBinaryWriter abw, Char obj)
        {
            abw.Write(obj);
        }
    }

