    class ByteSerialiser 
        : AssetTypeSerialiser<Byte>
    {
        internal ByteSerialiser () {}

        protected internal override Byte Read(AssetBinaryReader abr)
        {
            return abr.ReadByte ();
        }

        protected internal override void Write(AssetBinaryWriter abw, Byte obj)
        {
            abw.Write(obj);
        }
    }

