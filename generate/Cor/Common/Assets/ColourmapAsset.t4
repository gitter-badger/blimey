        public class ColourmapAsset
            : IAsset
        {
            public Int32 Width { get { return Data.GetLength (0); } }

            public Int32 Height { get { return Data.GetLength (1); } }

            /// <summary>
            /// A 32 BPP RGBA pixmap.
            /// </summary>
            public Rgba32[,] Data { get; set; }
        }

