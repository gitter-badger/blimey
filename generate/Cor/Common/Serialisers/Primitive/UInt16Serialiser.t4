    public class UInt16Serialiser
        : Serialiser<UInt16>
    {
        public UInt16Serialiser () {}

        public override UInt16 Read(BinaryReader abr)
        {
            return abr.ReadUInt16 ();
        }

        public override void Write(BinaryWriter abw, UInt16 obj)
        {
            abw.Write(obj);
        }
    }

